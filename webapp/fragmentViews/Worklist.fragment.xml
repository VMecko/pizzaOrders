<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:u="sap.ui.unified">
		<u:Menu>
			<u:MenuItem
				text="{i18n>addOrder}"
				select="addOrder"
				icon="sap-icon://add"/>
			<u:MenuItem
				text="{i18n>highOrder}"
				select="highestPrice"
				icon="sap-icon://opportunities"
				/>
			<u:MenuItem
				text="{i18n>geoMap}"
				select="showGeoMap"
				icon="sap-icon://globe"/>
			<u:MenuItem
				text="Weather"
				select="showWeather"
				icon="sap-icon://weather-proofing"/>
			<u:MenuItem
				text="{i18n>export}"
				icon="sap-icon://open-command-field"
				>
				<u:Menu>
					<u:MenuItem
						text="{i18n>PDF}"
						select="exportPDF" />
					<u:MenuItem
						text="{i18n>XLS}"
						select="exportXLS" />
				</u:Menu>
			</u:MenuItem>
			<u:MenuTextFieldItem
				label="Find"
				enabled="true"
				startsSection="true"
				icon="sap-icon://filter"
				select="handleTextFieldItemPress" />
		</u:Menu>

</core:FragmentDefinition>